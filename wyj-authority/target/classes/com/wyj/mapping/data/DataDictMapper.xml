<?xml version="1.0" encoding="UTF-8" ?>  
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >

<mapper namespace="com.wyj.dao.data.DataDictDao">
	<select id="getDataDictById" parameterType="java.lang.Long" resultType="com.wyj.entity.data.DataDict">
		SELECT * FROM B_BP_DATADICT WHERE
		dictId=#{dictId}
	</select>
	<insert id="saveDataDict" parameterType="com.wyj.entity.data.DataDict">
		INSERT INTO B_BP_DATADICT(dictCode,dictName,groupCode,remark,status,
		dictParentId) VALUES (#{dictCode},#{dictName},#{groupCode},#{remark},#{status},#{dictParentId})
	</insert>
	<delete id="removeDataDictById" parameterType="java.lang.Long">
		DELETE FROM B_BP_DATADICT WHERE
		dictId=#{dictId}
	</delete>

	<update id="updateDataDict" parameterType="com.wyj.entity.data.DataDict">
		UPDATE B_BP_DATADICT
		SET dictCode = #{dictCode},dictName=#{dictName},groupCode=#{groupCode},remark=#{remark},status=#{status},dictParentId=#{dictParentId}
	</update>
	<select id="listDataDict" resultMap="dataDictList">
		SELECT * FROM B_BP_DATADICT
	</select>
	<resultMap type="com.wyj.entity.data.DataDict" id="dataDictList">
		<result property="dictId" column="dictId" />
		<result property="dictCode" column="dictCode" />
		<result property="dictName" column="dictName" />
		<result property="groupCode" column="groupCode" />
		<result property="remark" column="remark" />
		<result property="status" column="status" />
		<result property="dictParentId" column="dictParentId" />
	</resultMap>
</mapper>